dbc00b9d4b68"],

Cell[TextData[Cell[BoxData[
 FormBox[GridBox[{
    {
     RowBox[{
      RowBox[{GridBox[{
         {
          RowBox[{
           RowBox[{
            SubscriptBox["f", "E"], "(", "E", ")"}], "=", 
           RowBox[{"2", "\[Sqrt]"}]}]}
        },
        GridBoxAlignment->{"Columns" -> {{"Center"}}}], 
       FractionBox[
        RowBox[{"\[IndentingNewLine]", "E"}], "\[Pi]"], 
       SuperscriptBox[
        RowBox[{"(", 
         FractionBox["1", "kT"], ")"}], 
        RowBox[{"3", "/", "2"}]], 
       RowBox[{"exp", "(", 
        RowBox[{
         RowBox[{"-", "E"}], "/", "kT"}], ")"}]}], ","}]}
   },

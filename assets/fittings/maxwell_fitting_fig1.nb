 CellChangeTimes->{{3.6383176364798326`*^9, 3.638317655649148*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"p", "[", 
   RowBox[{"E_", ",", "T_"}], "]"}], ":=", 
  RowBox[{"2", "*", 
   RowBox[{"Sqrt", "[", 
    RowBox[{"E", "/", "Pi"}], "]"}], "*", 
   RowBox[{
    RowBox[{"Exp", "[", 
     RowBox[{
      RowBox[{"-", "E"}], "/", "T"}], "]"}], "/", 
    RowBox[{"T", "^", 
     RowBox[{"(", 
      RowBox[{"3", "/", "2"}], ")"}], " ", 
     RowBox[{"(*", 
      RowBox[{"Maxwell", " ", "energy", " ", "distribution"}], 
      "*)"}]}]}]}]}]], "Input",
